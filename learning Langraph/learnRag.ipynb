    "sparse_index_name = \"sparse-stock-market-index\"\n",
    "\n",
    "# Using Pinecone Data Service for integrated embeddings\n",
    "from pinecone_datasets import Dataset\n",
    "\n",
    "# Create dataset with your documents\n",
    "dataset = Dataset.documents(\n",
    "    documents=pages_split,\n",
    "    text_field=\"chunk_text\"  # Field name for embedding\n",
    ")\n",
