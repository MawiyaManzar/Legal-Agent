    "# Upload to both indexes\n",
    "dataset.to_pinecone_index(\n",
    "    index_name=dense_index_name,\n",
    "    batch_size=100,\n",
    "    namespace=\"default\"\n",
    ")\n",
    "\n",
    "dataset.to_pinecone_index(\n",
    "    index_name=sparse_index_name, \n",
    "    batch_size=100,\n",
    "    namespace=\"default\"\n",
    ")"
